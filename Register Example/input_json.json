[
  {
    "id": 1,
    "title": "User details",
    "fields": [
      {
        "id": 1,
        "title": "First name",
        "apiKey": "first_name",
        "is_mandatory": true,
        "input_type": "text",
        "regex": "^[A-Za-z ]{3,16}$",
        "regex_alert": "alphabet should be minimum 3 char and maximum 16"
      },
      {
        "id": 2,
        "title": "Last name",
        "apiKey": "last_name",
        "is_mandatory": true,
        "input_type": "text",
        "regex": "^[A-Za-z ]{3,16}$",
        "regex_alert": "alphabet should be minimum 3 char and maximum 16"
      },
      {
        "id": 3,
        "title": "Gender",
        "apiKey": "gender",
        "is_mandatory": true,
        "input_type": "radio",
        "options": "Male,Female,Other"
      },
      {
        "id": 4,
        "title": "Mobile number",
        "apiKey": "mobile_number",
        "is_mandatory": true,
        "input_type": "number",
        "regex": "[0-9]{10}",
        "regex_alert": "should be in numeric 10 char"
      },
      {
        "id": 5,
        "title": "Email id",
        "apiKey": "email",
        "is_mandatory": true,
        "input_type": "text",
        "regex": "^[A-Z0-9a-z._%+-]+@[A-Za-z0-9.-]+\\.[A-Za-z]{2,64}$"
      }
    ]
  },
  {
    "id": 2,
    "title": "Experience details",
    "fields": [
      {
        "id": 6,
        "title": "Role",
        "apiKey": "role",
        "is_mandatory": true,
        "input_type": "text"
      },
      {
        "id": 7,
        "title": "Client name",
        "apiKey": "client_name",
        "is_mandatory": true,
        "input_type": "text"
      },
      {
        "id": 8,
        "title": "Year of experience",
        "apiKey": "year_experience",
        "is_mandatory": true,
        "input_type": "radio",
        "options": "0-2,2-5,5-10,10+"
      },
      {
        "id": 9,
        "title": "Technology based skils",
        "apiKey": "skils",
        "is_mandatory": true,
        "input_type": "check",
        "options": "Swift,Objective C,Kotlin,Java,Hybrid"
      }
    ]
  },
  {
    "id": 3,
    "title": "Office details",
    "fields": [
      {
        "id": 10,
        "title": "Manager name",
        "apiKey": "manager_name",
        "is_mandatory": true,
        "input_type": "text",
        "regex": "^[A-Za-z ]{3,30}$",
        "regex_alert": "alphabet should be minimum 3 char and maximum 30"
      },
      {
        "id": 11,
        "title": "HR name",
        "apiKey": "hr_name",
        "is_mandatory": false,
        "input_type": "text",
        "regex": "^[A-Za-z ]{3,30}$",
        "regex_alert": "alphabet should be minimum 3 char and maximum 30"
      },
      {
        "id": 12,
        "title": "Work location",
        "apiKey": "work_location",
        "is_mandatory": true,
        "input_type": "text"
      },
      {
        "id": 13,
        "title": "Set password",
        "apiKey": "user_password",
        "is_mandatory": true,
        "input_type": "password",
        "regex": "^(?=.*[a-z])(?=.*[A-Z])(?=.*\\d)(?=.*[$@$!%*?&#])[A-Za-z\\d$@$!%*?&#]{8,30}",
        "regex_alert": "minimum 8 and Maximum 30 characters at least 1 Uppercase Alphabet, 1 Lowercase Alphabet, 1 Number and 1 Special Character"
      }
    ]
  }
]
